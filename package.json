		"@types/eslint": "^9.6.1",
		"@types/opentype.js": "^1.3.8",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.11.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"svelte": "^5.1.3",
		"svelte-check": "^4.0.5",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.6.3",
